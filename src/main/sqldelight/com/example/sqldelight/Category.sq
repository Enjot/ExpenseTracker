CREATE TABLE category (
  name TEXT NOT NULL PRIMARY KEY UNIQUE,
  isDeleted INTEGER DEFAULT FALSE NOT NULL
);

CREATE INDEX categoryName ON category(name);

selectAll:
SELECT *
FROM category
ORDER BY name;

select:
SELECT name
FROM category
WHERE category.name = ?;

insert:
INSERT INTO category(name)
VALUES (name = ?);

delete:
UPDATE category
SET isDeleted = TRUE
WHERE category.name = ?;

