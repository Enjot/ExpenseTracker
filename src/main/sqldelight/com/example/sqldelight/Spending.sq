CREATE TABLE spending (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    amount REAL NOT NULL,
    date TEXT NOT NULL
--     categoryId
);

CREATE INDEX spendingDate ON spending(date);

selectAll:
SELECT *
FROM spending
ORDER BY id DESC ;


insert:
INSERT INTO spending(name, amount, date)
VALUES (?,?,?);

delete:
DELETE FROM spending
WHERE id = ?;